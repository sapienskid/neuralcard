    "@typescript-eslint/parser": "5.2.0",
    "builtin-modules": "3.3.0",
    "esbuild": "0.17.3",
    "obsidian": "^1.5.8",
    "tslib": "2.4.0",
    "typescript": "4.7.4"
  },
  "dependencies": {
    "chart.js": "^4.5.1",
    "crypto-js": "^4.2.0",
    "ts-fsrs": "^4.1.1"
