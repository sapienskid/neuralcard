		"url": "https://github.com/sapienskid/neuralcard-flashcards.git"
	},
	"devDependencies": {
		"@types/node": "^18.0.0",
		"@typescript-eslint/eslint-plugin": "^5.29.0",
		"@typescript-eslint/parser": "^5.29.0",
		"builtin-modules": "3.3.0",
		"esbuild": "0.17.3",
		"obsidian": "^1.4.0",
		"tslib": "2.4.0",
		"typescript": "^4.7.4"
	}
